{
  "name": "manojepam",
  "version": "1.0.0",
  "description": "This project demonstrates end-to-end testing using Playwright with Cucumber for BDD-style test automation.",
  "main": "index.js",
  "scripts": {
    "pretest": "rimraf reports && mkdir reports",
    "test": "cucumber-js -p default --format json:reports/cucumber-report.json --format summary",
    "report": "node generate-html-report.js",
    "test:tags": "cucumber-js -p default --tags \"@%npm_config_tag%\" --format json:reports/cucumber-report.json --format summary && ts-node report-generator.ts",
    "test:valid": "cucumber-js -p default --tags @valid --format json:reports/cucumber-report.json --format summary && ts-node report-generator.ts",
    "test:invalid": "cucumber-js -p default --tags @invalid --format json:reports/cucumber-report.json --format summary && ts-node report-generator.ts",
    "test:locked": "cucumber-js -p default --tags @locked --format json:reports/cucumber-report.json --format summary && ts-node report-generator.ts",
    "test:all": "cucumber-js -p default --format json:reports/cucumber-report.json --format summary && ts-node report-generator.ts",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "clean": "rimraf reports && mkdir reports"
  },
