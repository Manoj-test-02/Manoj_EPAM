{
  "name": "rally-testcase-generator",
  "displayName": "Rally Testcase Generator",
  "description": "Fetch Rally description and generate Gherkin test cases using ChatGPT",
  "version": "1.0.0",
  "publisher": "your-name",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:rallyTestcaseGenerator.start"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "rallyTestcaseGenerator.start",
        "title": "Rally: Generate Gherkin Test Cases"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -p ./"
  },
  "devDependencies": {
    "@types/node": "^24.2.0",
    "@types/vscode": "^1.70.0",
    "tslint": "^6.1.3",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "axios": "^1.11.0"
  }
}
